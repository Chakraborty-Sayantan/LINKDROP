{
  "version": 2,
  "builds": [
    {
      "src": "linkdrop_backend/linkdrop/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "linkdrop_backend/linkdrop/wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}